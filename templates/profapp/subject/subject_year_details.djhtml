{% extends "base.djhtml" %}

{% block title %}
<p>{{ subject_year.name }} - {{ subject_year.year }} {{ subject_year.students }} </p>
{% endblock %}

{% block content %}
<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd>{{ subject_year.name }}</dd>

  <dt>Year</dt>
  <dd>{{ subject_year.year }}</dd>

  <dt>Students</dt>
  <dd>{{ subject_year.students }}</dd>

</dl>
{% endblock %}
